{
    "name": "limesurvey/webhook",
    "description": "LimeSurvey plugin that sends webhook POST requests after survey completion",
    "type": "limesurvey-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Stefan Verweij",
            "email": "stefan@evently.nl"
        },
        {
            "name": "IrishWolf"
        },
        {
            "name": "Alex Righetto"
        },
        {
            "name": "Tom Riat"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "classmap": [
            "LimeSurveyWebhook.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LimeSurveyWebhook\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "lint": "phpcs",
        "lint-fix": "phpcbf"
    }
}
